<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kcidea.erms.dao.task.ErmsTaskRecordDao">
    <select id="findOneByTaskId" resultType="com.kcidea.erms.domain.task.ErmsTaskRecord">
        select * from erms_task_record where task_id = #{taskId}
    </select>
    <delete id="deleteByTaskId">
        delete from erms_task_record where task_id = #{taskId}
    </delete>
</mapper>
