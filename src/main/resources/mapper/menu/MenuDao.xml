<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kcidea.erms.dao.menu.MenuDao">

    <select id="findAllMenu" resultType="com.kcidea.erms.model.menu.MenuModel">
        SELECT id,name,model_name,path,level,parent_id,show_flag,
               select_flag,insert_flag,update_flag,delete_flag,import_flag,export_flag,other_flag
        FROM menu
    </select>
</mapper>
