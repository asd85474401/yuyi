<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kcidea.erms.dao.ers.SchoolDao">

    <select id="findStartYearById" resultType="java.lang.Integer">
        SELECT start_year FROM school WHERE id = #{id}
    </select>
    <select id="findSchoolNameBySid" resultType="java.lang.String">
        select name from school where id = #{sid}
    </select>
</mapper>
