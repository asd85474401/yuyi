<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kcidea.erms.dao.fund.PayRemindDao">

    <select id="findOneBySid" resultType="com.kcidea.erms.domain.fund.PayRemind">
        SELECT id, s_id, remind_day, type, remark FROM pay_remind WHERE s_id = #{sid}
    </select>
</mapper>
